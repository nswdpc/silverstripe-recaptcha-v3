{
    "name": "nswdpc/silverstripe-recaptcha-v3",
    "description": "Recaptcha V3 verification for Silverstripe framework, including a form field, spam protector and verification support",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "recaptcha",
        "v3",
        "verify",
        "spam",
        "spamprotection"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/spamprotection": "^4"
    },
    "require-dev": {
        "cambis/silverstripe-rector": "^2",
        "cambis/silverstan": "^2",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^2",
        "rector/rector": "^2",
        "nswdpc/ci-files": "dev-v-4",
        "vlucas/phpdotenv": "^5",
        "phpstan/phpstan-phpunit": "^2"
    },
    "suggest": {
        "nswdpc/silverstripe-recaptcha-v3-userforms": "Adding reCAPTCHA v3 field via silverstripe/userforms"
    },
    "authors": [
        {
            "name": "James Ellis",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NSWDPC\\SpamProtection\\Tests\\": [
                "tests/"
            ],
            "NSWDPC\\SpamProtection\\": [
                "src/Controllers",
                "src/Extensions",
                "src/Forms",
                "src/Models",
                "src/Services",
                "src/Traits"
            ]
        }
    },
    "extra": {
        "expose": [
            "client/static"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
